          },
          "format": "table",
          "rawSql": "WITH last_open AS (SELECT time as open_time FROM events WHERE site = '$site' AND event_type = 'gateway.gate' AND data->>'src' = 'rs485' AND data->>'state' = 'open' ORDER BY time DESC LIMIT 1), last_close AS (SELECT time as close_time FROM events WHERE site = '$site' AND event_type = 'gateway.gate' AND data->>'src' = 'rs485' AND data->>'state' = 'closed' ORDER BY time DESC LIMIT 1) SELECT COUNT(*) as exits FROM events e, last_open o, last_close c WHERE e.site = '$site' AND e.event_type = 'gateway.events' AND e.data->>'t' = 'line_cross' AND UPPER(e.data->>'z') LIKE '%EXIT%' AND e.time >= o.open_time AND (o.open_time > c.close_time OR e.time <= c.close_time);",
          "refId": "A"
        }
      ],
      "title": "Exits (cycle)",
      "type": "stat"
    },
