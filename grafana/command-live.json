          "datasource": { "type": "grafana-postgresql-datasource", "uid": "P40AE60E18F02DE32" },
          "format": "time_series",
          "rawSql": "WITH hours AS (SELECT generate_series(0, 23) as hour), today_data AS (SELECT EXTRACT(HOUR FROM time AT TIME ZONE 'Atlantic/Reykjavik')::int as hour, COUNT(*) as cnt FROM person_journeys WHERE site = 'netto' AND time >= CURRENT_DATE AND outcome IS NOT NULL GROUP BY 1), yesterday_data AS (SELECT EXTRACT(HOUR FROM time AT TIME ZONE 'Atlantic/Reykjavik')::int as hour, COUNT(*) as cnt FROM person_journeys WHERE site = 'netto' AND time >= CURRENT_DATE - INTERVAL '1 day' AND time < CURRENT_DATE AND outcome IS NOT NULL GROUP BY 1) SELECT (CURRENT_DATE + (h.hour || ' hours')::interval) AT TIME ZONE 'Atlantic/Reykjavik' as time, COALESCE(t.cnt, 0) as today, COALESCE(y.cnt, 0) as yesterday FROM hours h LEFT JOIN today_data t ON h.hour = t.hour LEFT JOIN yesterday_data y ON h.hour = y.hour ORDER BY h.hour;",
          "refId": "A"
        }
      ],
      "title": "Today vs Yesterday",
